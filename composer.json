{
    "name": "atom-azimov/uploader",
    "description": "AtomUploader is a simple yet flexible PHP library for file uploads via ORM embeddable objects, ORM entities, etc.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Elbek Azimov",
            "email": "atom.azimov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Atom\\Uploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Atom\\Uploader\\": "spec/",
            "App\\": "example-app/src/"
        }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpspec/phpspec": "^2.4",
        "phpunit/phpunit": "^5.1",
        "henrikbjorn/phpspec-code-coverage": "^2.0.2",
        "league/flysystem": "^1.0",
        "satooshi/php-coveralls": "^1.0",
        "mikey179/vfsStream": "^1.6",
        "twistor/flysystem-stream-wrapper": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
